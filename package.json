{
  "name": "@piwikpro/nuxt-piwik-pro",
  "private": false,
  "version": "1.0.0",
  "description": "Dedicated Piwik PRO library that helps with implementing Piwik PRO Tag Manager and the Piwik PRO tracking client in Nuxt.js applications.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "tsc -b",
    "build:docs": "typedoc && concat-md --decrease-title-levels --start-title-level-at=2 docs_raw > README.md && node scripts/postProcessDocs.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nuxt",
    "piwik-pro",
    "analytics",
    "plugin"
  ],
  "author": "Piwik Pro Integration Team <integrations@piwik.pro>",
  "license": "MIT",
  "dependencies": {
    "@piwikpro/vue-piwik-pro": "^1.2.0"
  },
  "peerDependencies": {
    "nuxt": ">=3.0.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/types": "^2.17.3",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "concat-md": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3"
  }
}
